<div class="modal fade" id="usrimg-cropp" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
        <h4 class="modal-title" id="exampleModalLabel">Image Cropper</h4>
      </div>
      <div class="modal-body">
        <div class="container">
          <div class="row">
            <div class="col-md-12">
              <!-- <h3 class="page-header">Demo:</h3> -->
              <div class="img-container">
                <div >
                  <input id="company_background_image_crop_x" type="hidden" name="company[background_image_crop_x]" value="0">
                  <input id="company_background_image_crop_y" type="hidden" name="company[background_image_crop_y]" value="0">
                  <input id="company_background_image_crop_w" type="hidden" name="company[background_image_crop_w]" value="200">
                  <input id="company_background_image_crop_h" type="hidden" name="company[background_image_crop_h]" value="200">
                </div>
                <img id="image" src="data:image/jpeg;base64," alt="Picture" alt="Picture">
              </div>
            </div>
            <div class="col-md-12">
              <!-- <h3 class="page-header">Preview:</h3> -->
              <div class="docs-preview clearfix">
                <div class="img-preview preview-lg"></div>
              </div>
            </div>
          </div>          
        </div>
      </div>
      <div class="modal-footer">
        <div class="row">
          <div class="col-md-12 docs-buttons">
            <!-- Show the cropped image in modal -->
            <a class="hide" id="download" href="javascript:void(0);" download="cropped.jpg" ></a>
            <div class="btn-group btn-group-crop">
            <button type="button" class="btn btn-primary crop-button" data-method="getCropBoxData" data-option data-target="#putData">
              <span class="docs-tooltip" data-toggle="tooltip" title="$().cropper(&quot;getCropBoxData&quot;)">
                Crop
              </span>
            </button>        
            <input type="hidden" class="form-control" id="putData" name="crop" id="crop" placeholder="Get data to here or set data with this value">
            </div>
          </div>
        </div>         
      </div>
    </div>
  </div>
</div>